{
    "name": "@slimio/pretty-json",
    "version": "1.3.0",
    "description": "Stdout JSON in your terminal",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && jest --coverage",
        "coverage": "codecov",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Pretty-JSON.git"
    },
    "keywords": [
        "pretty",
        "json",
        "beautiful",
        "format",
        "stdout",
        "terminal"
    ],
    "files": [
        "index.js",
        "index.d.ts",
        "src/utils.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Pretty-JSON/issues"
    },
    "homepage": "https://github.com/SlimIO/Pretty-JSON#readme",
    "dependencies": {
        "@slimio/is": "^1.5.1",
        "kleur": "^4.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/psp": "^0.11.1",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "jsdoc": "^3.6.4",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
