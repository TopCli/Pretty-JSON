{
    "name": "@slimio/pretty-json",
    "version": "1.3.0",
    "description": "Stdout JSON in your terminal",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && jest --coverage",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Pretty-JSON.git"
    },
    "keywords": [
        "pretty",
        "json",
        "beautiful",
        "format",
        "stdout",
        "terminal"
    ],
    "files": [
        "index.js",
        "index.d.ts",
        "src/utils.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Pretty-JSON/issues"
    },
    "homepage": "https://github.com/SlimIO/Pretty-JSON#readme",
    "dependencies": {
        "@slimio/is": "^1.5.1",
        "kleur": "^3.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.2",
        "@slimio/psp": "^0.9.2",
        "cross-env": "^6.0.3",
        "eslint": "^6.6.0",
        "husky": "^3.0.9",
        "jest": "^25.2.3",
        "jsdoc": "^3.6.3",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
